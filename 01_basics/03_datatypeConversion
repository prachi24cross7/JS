let score = true

console.log(typeof score);

let valueInNumber = Number(score)

console.log(typeof valueInNumber);
console.log(valueInNumber);

//"33" = 33
//"33abc" =NaN
//null = 0
//undefined = NaN
//true = 1
//false = 0

let isLoggedIn = ""
let booleanLoggedIn = Boolean(isLoggedIn)

console.log(booleanLoggedIn)

//1 => true 0 => false
//"Prachi" => true
//"" => false